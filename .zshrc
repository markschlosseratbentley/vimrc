export GIT_EDITOR="/Applications/MacVim.app/Contents/bin/mvim -f"
alias vi="/Applications/MacVim.app/Contents/bin/mvim"
alias vt="/Applications/MacVim.app/Contents/bin/mvim --remote-silent-tab"
alias mktags="ctags --extra=+f -R --exclude=.git --exclude=node_modules --exclude='*.js' --exclude='*.d.ts'"

